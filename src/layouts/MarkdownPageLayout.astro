---
import BaseLayout from "./BaseLayout.astro";

const { frontmatter } = Astro.props;
---

<BaseLayout description={frontmatter.description} pageTitle={frontmatter.title}>
  <article class="m-auto w-1/2 text-justify">
    <h1 class="mb-4 text-center text-3xl font-bold">{frontmatter.title}</h1>
    <slot />
  </article>
</BaseLayout>
